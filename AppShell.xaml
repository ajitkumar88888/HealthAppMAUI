<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiAppHealth.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiAppHealth"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Title="SignUpPage"
        ContentTemplate="{DataTemplate local:SignUpPage}"
        Route="SignUpPage" />
    
    <ShellContent
        Title="OTPPage"
        ContentTemplate="{DataTemplate local:OTPPage}"
        Route="OTPPage" />

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    
    <ShellContent
        Title="SetUpProfilePage"
        ContentTemplate="{DataTemplate local:SetUpProfilePage}"
        Route="SetUpProfilePage" />
</Shell>
